diff --git a/CMakeLists.txt b/CMakeLists.txt
new file mode 100644
index 00000000..d01d8845
--- /dev/null
+++ b/CMakeLists.txt
@@ -0,0 +1,38 @@
+cmake_minimum_required(VERSION 3.10)
+
+project(libdb VERSION 18.1.40 LANGUAGES C)
+
+set(EXTERNAL_LIBDB "${CMAKE_SOURCE_DIR}/build_unix")
+set(DB_LIB "${EXTERNAL_LIBDB}/.libs/libdb-18.1.a")
+
+add_custom_command(
+    OUTPUT ${EXTERNAL_LIBDB}/Makefile
+    COMMAND ${CMAKE_COMMAND} -E echo "Configuring Berkeley DB"
+    COMMAND cd ${EXTERNAL_LIBDB} && CPPFLAGS=-fPIC ../dist/configure
+            --with-cryptography=no
+            --disable-queue
+            --disable-heap
+            --disable-partition
+            --disable-mutexsupport
+            --disable-replication
+            --disable-verify
+            --disable-statistics
+            ac_cv_func_pthread_yield=no
+    WORKING_DIRECTORY ${EXTERNAL_LIBDB}
+    COMMENT "Configuring Berkeley DB with custom options"
+)
+
+add_custom_target(build_libdb ALL
+    COMMAND ${CMAKE_MAKE_PROGRAM} -C ${EXTERNAL_LIBDB} libdb.a
+    DEPENDS ${EXTERNAL_LIBDB}/Makefile
+    WORKING_DIRECTORY ${EXTERNAL_LIBDB}
+    COMMENT "Compiling Berkeley DB (libdb-18.1.a)"
+)
+
+add_library(libdb STATIC IMPORTED GLOBAL)
+set_target_properties(libdb PROPERTIES IMPORTED_LOCATION ${DB_LIB})
+
+file(GLOB DB_HEADERS "${EXTERNAL_LIBDB}/*.h")
+
+install(FILES ${DB_HEADERS} DESTINATION include/libdb)
+install(FILES ${DB_LIB} DESTINATION lib)
