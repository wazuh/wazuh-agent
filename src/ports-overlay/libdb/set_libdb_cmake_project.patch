-{
-	db_indx_t i;
-	u_int32_t sz;
-	sz = 0;
-
-	for (i = 0; i < NUM_ENT(h); i += O_INDX) {
-		/* Ignore deleted items. */
-		if (B_DISSET(GET_BKEYDATA(dbp, h, i)->type))
-			continue;
-		sz += db_tuner_item_space(dbp, h, i);
