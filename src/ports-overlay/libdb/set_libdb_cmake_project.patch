-#define	ENV_LEAVE(env, ip) do {						\
-	if ((ip) != NULL) {	\
-		DB_ASSERT((env), (ip)->dbth_state == THREAD_ACTIVE  ||	\
-		    (ip)->dbth_state == THREAD_FAILCHK);		\
-		(ip)->dbth_state = THREAD_OUT;				\
-	}								\
-} while (0)
